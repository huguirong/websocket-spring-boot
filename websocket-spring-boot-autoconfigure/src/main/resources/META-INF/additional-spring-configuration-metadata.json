    {
      "name": "app.websocket.user-topic-suffix",
      "type": "java.lang.String",
      "sourceType": "com.ugrong.framework.ws.config.WsProperties",
      "defaultValue": "/queue",
      "description": "给指定用户发送（一对一）消息的主题后缀."
    },
    {
      "name": "app.websocket.principal-id-field-name",
      "type": "java.lang.String",
      "sourceType": "com.ugrong.framework.ws.config.WsProperties",
      "defaultValue": "userId",
      "description": "主体对象的id字段名."
    },
    {
      "name": "app.websocket.system-user-id",
      "type": "java.lang.String",
      "sourceType": "com.ugrong.framework.ws.config.WsProperties",
